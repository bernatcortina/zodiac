  '/Users/Bernat/Pyramid/zodiac/eggs/pytest_cov-1.6-py2.7.egg',
  '/Users/Bernat/Pyramid/zodiac/eggs/cov_core-1.7-py2.7.egg',
  '/Users/Bernat/Pyramid/zodiac/eggs/py-1.4.18-py2.7.egg',
  '/Users/Bernat/Pyramid/zodiac/eggs/beautifulsoup4-4.3.2-py2.7.egg',
  '/Users/Bernat/Pyramid/zodiac/eggs/waitress-0.8.7-py2.7.egg',
  '/Users/Bernat/Pyramid/zodiac/eggs/six-1.4.1-py2.7.egg',
  '/Users/Bernat/Pyramid/zodiac/eggs/coverage-3.7-py2.7-macosx-10.9-intel.egg',
  '/Library/Python/2.7/site-packages',
  '/Users/Bernat/Pyramid/zodiac/bin',
  '/Users/Bernat/Pyramid/zodiac/parts/zodiac',
  '/Users/Bernat/Pyramid/zodiac/parts/google_appengine',
